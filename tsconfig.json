{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "*": ["./src/*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true, 
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowJs": true
  },
  "include": ["src", "public/index.js", "public", "index.js", "media/jsonTreeMediaConverter.ts", "media/csvMediaConverter.js"],
  "exclude": ["node_modules", "dist"]
}
